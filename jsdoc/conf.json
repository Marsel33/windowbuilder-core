{
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "jsdoc/metadata.jsdoc",
      "jsdoc/paper.jsdoc",
      "jsdoc/enums.js",
      "dist/init.js",
      "src/geometry/"
    ],
    "exclude": [
      "./src/geometry/contour_nested.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "systemName": "wb-core",
    "systemSummary": "Ядро Заказа дилера, безбумажки и параметрика",
    "copyright": "© 2016-2022 Oknosoft Lab <info@oknosoft.ru> (https://oknosoft.ru)",
    "navMembers": [
      {"kind": "class", "title": "Classes", "summary": "Все классы библиотеки"},
      {"kind": "namespace", "title": "Namespaces", "summary": "Пространства имён"},
      {"kind": "global", "title": "Globals", "summary": "Глобальные переменные"},
      {"kind": "tutorial", "title": "Tutorials", "summary": "Руководства и примеры"}
    ],
    "inlineNav": true,
    "referenceTitle": "",
    "disableSort": false,
    "collapse": true,
    "cleverLinks": false,
    "monospaceLinks": false,
    "resources": {
      "metadata.js<br/>": "https://github.com/oknosoft/metadata.js/tree/develop/packages",
      "paper.js": "http://paperjs.org/reference/"
    }
  },
  "docdash": {
    "meta": {
      "title": "Ядро 'Заказа дилера'",
      "description": "Ядро 'Заказа дилера'",
      "keyword": "metadata, order dealer, cad, crm, window builder, windows calculator"
    },
    "search": true,
    "sort": true,
    "collapse": true,
    "menu": {
      "Заказ дилера": {
        "href":"https://github.com/oknosoft/windowbuilder/wiki",
        "target":"_blank",
        "class":"menu-item",
        "id":"website_link"
      }
    },
    "sectionOrder": [
      "Tutorials",
      "Namespaces",
      "Classes",
      "Externals",
      "Modules",
      "Events",
      "Mixins",
      "Interfaces"
    ]
  },
  "opts": {
    "destination": "./jsdoc/wbcore/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/foodoc/template",
    "tutorials": "./jsdoc/tutorials/",
    "readme": "./README.md"
  }
}
