{
  "plugins": [
    "plugins/markdown"
  ],
  "source": {
    "include": [
      "jsdoc/metadata.jsdoc",
      "jsdoc/paper.jsdoc",
      "jsdoc/enums.js",
      "dist/init.js",
      "src/geometry/"
    ],
    "exclude": [
      "./src/geometry/contour_nested.js"
    ],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "systemName": "wb-core",
    "systemLogo": "./jsdoc/tutorials/imgs/flask.png",
    "favicon": "./jsdoc/tutorials/imgs/favicon.ico",
    "dateFormat": "DD.MM.YYYY",
    "systemSummary": "Ядро Заказа дилера, безбумажки и параметрика",
    "copyright": "© 2016-2022 Oknosoft Lab <info@oknosoft.ru> (https://oknosoft.ru)",
    "navMembers": [
      {"kind": "class", "title": "Классы", "summary": "Все классы библиотеки"},
      {"kind": "namespace", "title": "Зависимости", "summary": "Пространства имён"},
      {"kind": "tutorial", "title": "Руководства", "summary": "Руководства и примеры"}
    ],
    "inlineNav": true,
    "referenceTitle": "",
    "disableSort": false,
    "collapse": true,
    "cleverLinks": false,
    "monospaceLinks": false,
    "resources": {
      "metadata.js": "https://github.com/oknosoft/metadata.js/tree/develop/packages",
      "paper.js": "http://paperjs.org/reference/"
    }
  },
  "opts": {
    "destination": "./jsdoc/wbcore/",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "template": "./node_modules/foodoc/template",
    "tutorials": "./jsdoc/tutorials/",
    "readme": "./README.md"
  }
}
